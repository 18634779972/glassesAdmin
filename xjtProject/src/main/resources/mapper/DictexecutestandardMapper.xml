<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xjt.dao.master.DictexecutestandardDao" >

    <resultMap id="BaseResultMap" type="com.xjt.entity.Dictexecutestandard" >
        <id column="ExecuteStandard" property="executestandard" jdbcType="VARCHAR" />
    </resultMap>

    <!--批量删除执行标准-->
    <delete id="deleteByPrimaryKey" parameterType="java.util.List" >
        delete from dictexecutestandard
        where ExecuteStandard in
        <foreach collection="standards" item="executestandard" open="(" close=")" separator=",">
            #{executestandard,jdbcType=VARCHAR}
        </foreach>

    </delete>

    <insert id="insert" parameterType="com.xjt.entity.Dictexecutestandard" >
        insert into dictexecutestandard (ExecuteStandard)
        values (#{executestandard,jdbcType=VARCHAR})
    </insert>

    <insert id="insertSelective" parameterType="com.xjt.entity.Dictexecutestandard" >
        insert into dictexecutestandard
        <trim prefix="(" suffix=")" suffixOverrides="," >
            <if test="executestandard != null" >
                ExecuteStandard,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides="," >
            <if test="executestandard != null" >
                #{executestandard,jdbcType=VARCHAR},
            </if>
        </trim>
    </insert>
    <insert id="insertBatch" parameterType="java.util.List">
         insert into dictexecutestandard (ExecuteStandard)
        values
        <foreach collection="standards" item="executestandard" separator=",">
            (#{executestandard,jdbcType=VARCHAR})
        </foreach>
    </insert>
    <!--查看执行标准-->
    <select id="queryAllStandards" parameterType="com.xjt.entity.Dictexecutestandard" resultMap="BaseResultMap">
        select ExecuteStandard from dictexecutestandard
          <where>
              <if test="executestandard != null" >
                  and ExecuteStandard like '%'+#{executestandard,jdbcType=VARCHAR}+'%',
              </if>
          </where>
    </select>
</mapper>