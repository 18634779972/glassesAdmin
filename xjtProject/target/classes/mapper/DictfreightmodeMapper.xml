<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xjt.dao.master.DictfreightmodeDao" >

    <resultMap id="BaseResultMap" type="com.xjt.entity.Dictfreightmode" >
        <id column="FreightMode" property="freightmode" jdbcType="VARCHAR" />
    </resultMap>

    <delete id="deleteByPrimaryKey" parameterType="java.util.List" >
        delete from dictfreightmode
        where FreightMode in
        <foreach collection="modes" item="freightmode" separator="," open="(" close=")">
            #{freightmode,jdbcType=VARCHAR}
        </foreach>


    </delete>

    <insert id="insert" parameterType="com.xjt.entity.Dictfreightmode" >
        insert into dictfreightmode (FreightMode)
        values (#{freightmode,jdbcType=VARCHAR})
    </insert>

    <insert id="insertSelective" parameterType="com.xjt.entity.Dictfreightmode" >
        insert into dictfreightmode
        <trim prefix="(" suffix=")" suffixOverrides="," >
            <if test="freightmode != null" >
                FreightMode,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides="," >
            <if test="freightmode != null" >
                #{freightmode,jdbcType=VARCHAR},
            </if>
        </trim>
    </insert>
    <insert id="insertBatch" parameterType="java.util.List">
         insert into dictfreightmode (FreightMode)
        values
        <foreach collection="modes" item="freightmode" separator="," >
            (#{freightmode,jdbcType=VARCHAR})
        </foreach>
    </insert>
    <select id="queryAllModes" parameterType="com.xjt.entity.Dictfreightmode" resultMap="BaseResultMap">
        select FreightMode from dictfreightmode
        <where>
            <if test="freightmode != null">
                and FreightMode like '%'+#{freightmode,jdbcType=VARCHAR}+'%'
            </if>
        </where>
    </select>
</mapper>