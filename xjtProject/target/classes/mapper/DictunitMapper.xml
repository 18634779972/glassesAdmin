<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xjt.dao.master.DictunitDao" >

    <resultMap id="BaseResultMap" type="com.xjt.entity.Dictunit" >
        <id column="Unit" property="unit" jdbcType="VARCHAR" />
    </resultMap>
    <!--批量删除-->
    <delete id="deleteByPrimaryKey" parameterType="java.util.List" >
        delete from dictunit
        where Unit in
        <foreach collection="units" item="unit" open="(" close=")" separator=",">
            #{unit,jdbcType=VARCHAR}
        </foreach>

    </delete>

    <insert id="insert" parameterType="com.xjt.entity.Dictunit" >
        insert into dictunit (Unit)
        values (#{unit,jdbcType=VARCHAR})
    </insert>

    <insert id="insertSelective" parameterType="com.xjt.entity.Dictunit" >
        insert into dictunit
        <trim prefix="(" suffix=")" suffixOverrides="," >
            <if test="unit != null" >
                Unit,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides="," >
            <if test="unit != null" >
                #{unit,jdbcType=VARCHAR},
            </if>
        </trim>
    </insert>
    <!--批量添加-->
    <insert id="insertUnitsBatch" parameterType="java.util.List">
        insert into dictunit (Unit)
        values
        <foreach collection="units" item="unit" index="index" separator=",">
            ( #{unit,jdbcType=VARCHAR})
        </foreach>
    </insert>
    <!--查询-->
    <select id="queryAllUnits" parameterType="com.xjt.entity.Dictunit" resultMap="BaseResultMap">
        select Unit from dictunit
          <where>
              <if test="unit != null">
                  and Unit like '%'+#{unit}+'%'
              </if>
          </where>
    </select>
</mapper>